definition {

    macro clickSimulation {

        Click(locator1 = "MyPaths#BOTAO_SIMULATION");
        Pause(locador1 = "3000");

    }

    macro clickCustom {

        Click(locator1 = "MyPaths#BOTAO_CUSTOM");
        Pause(locador1 = "3000");
    }

    macro clickHeight {

        Type(
            
            locator1 = "MyPaths#BOTAO_HEIGHT",
            value1 = "1920");
            Pause(locador1 = "3000");

    }

    macro clickWidth {

        Type(
            locator1 = "MyPaths#BOTAO_WIDTH",
            value1 = "1080");
            Pause(locador1 = "3000");
    }

    macro validarHeight {

        AssertTextEquals(
            locator1 = "MyPaths#BOTAO_HEIGHT",
            value1 = "1920");

    }

    macro validarWidth {

        Type(
            locator1 = "MyPaths#BOTAO_WIDTH",
            value1 = "1080");
            Pause(locador1 = "3000");
    }



}